{
  "manifest_version": 2,
  "name": "Qiniu 4 cdnjs",
  "version": "1.0.1",
  "description": "Replace the links of cdnjs with Qiniu.",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "https://cdnjs.com/*"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}